{
  "name": "domestique",
  "version": "1.0.0",
  "description": "Fitness MCP server integrating Intervals.icu, Whoop, and TrainerRoad",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/resources/*.md dist/resources/",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "whoop:auth": "node dist/scripts/whoop-oauth.js",
    "whoop:auth:dev": "tsx src/scripts/whoop-oauth.ts",
    "test:clients": "node dist/scripts/test-clients.js"
  },
  "keywords": [
    "mcp",
    "fitness",
    "intervals.icu",
    "whoop",
    "trainerroad"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chrono-node": "^2.9.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express": "^5.2.1",
    "node-ical": "^0.22.1",
    "redis": "^5.10.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
